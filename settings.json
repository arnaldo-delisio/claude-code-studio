{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "model": "opusplan",
  "statusLine": {
    "type": "command",
    "command": "node statusline-context-tracker.js"
  },
  "feedbackSurveyState": {
    "lastShownTime": 1754067711350
  },
  "permissions": {
    "allow": [
      "Bash(osascript:*)",
      "Bash(afplay:*)"
    ]
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Glass.aiff",
            "timeout": 5
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "TodoWrite",
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Tink.aiff",
            "timeout": 5
          }
        ]
      }
    ]
  }
}